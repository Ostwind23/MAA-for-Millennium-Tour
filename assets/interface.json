                    "name": "不购买种子"
                },
                {
                    "name": "购买全部种子",
                    "pipeline_override": {
                        "Seed_Mark": {
                            "attach": {
                                "龙鳞果": true,
                                "青海莓": true,
                                "千蝶菇": true,
                                "黄金葡萄": true,
                                "连叶卷心菜": true,
                                "拇指玉米": true,
