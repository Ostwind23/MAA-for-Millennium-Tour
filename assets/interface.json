            "type": "select",
            "cases": [
                {
                    "name": "本勒关",
                    "pipeline_override": {
                        "chooseOwnmission": {
                            "template": "money mission.png"
                        }
                    }
                },
                {
                    "name": "经验关",
                    "pipeline_override": {
                        "chooseOwnmission": {
                            "template": "exp mission.png"
                        }
                    }
                },
                {
                    "name": "技能书关",
                    "pipeline_override": {
                        "chooseOwnmission": {
                            "template": "skill mission.png"
                        }
                    }
                }
            ]
        },
        "资源关关卡选择设置": {
            "type": "select",
            "cases": [
                {
                    "name": "自动选择目前能打的最高关次（仅限未打完全部关卡的人选择）",
                    "pipeline_override": {
                        "scanmission": {
                            "index": -2
                        }
                    }
                },
                {
                    "name": "选择最后一关",
                    "pipeline_override": {
                        "scanmission": {
                            "index": -1
                        }
                    }
                }
            ]
        },
        "骚灵之境关卡选择设置": {
            "type": "select",
