            "type": "select",
            "cases": [
                {
                    "name": "表界",
                    "pipeline_override": {
                        "beginAutoPush": {
                            "action": "DoNothing"
                        }
                    }
                },
                {
                    "name": "里界",
                    "pipeline_override": {
