                        "scanGhostmission": {
                            "index": -2
                        }
                    }
                },
                {
                    "name": "选择最后一关",
                    "pipeline_override": {
                        "scanGhostmission": {
                            "index": -1
                        }
                    }
                }
            ]
        },
